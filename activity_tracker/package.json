{
  "name": "activity-tracker",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"nodemon --watch src --ext ts --exec \\\"ts-node src/server.ts\\\"\" \"pnpm -C client dev\"",
    "start": "node dist/server.js",
    "start:dev": "ts-node src/server.ts",
    "build": "pnpm run build:server && pnpm run build:client",
    "build:server": "tsc",
    "build:client": "pnpm -C client build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "sql.js": "^1.13.0",
    "http-proxy-middleware": "^3.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
